@page "/login"

<PageTitle>Login</PageTitle>

<div class="login-box">
	<p>Login</p>
	<form>
		<div class="user-box">
			<label>Email:</label>
			<input required="" name="" type="text"/>
		</div>
		<div class="user-box">
			<label>Password:</label>
			<input required="" name="" type="password"/>
		</div>
		<div class="user-box">
			<label>Access Key:</label>
			<input required="" name="" type="text"/>
		</div>
		<button>Submit</button>
	</form>
	<p>Har du ikke en konto?<a href="">Sign up!</a></p>
</div>

