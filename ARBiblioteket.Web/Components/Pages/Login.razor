@page "/login"

<PageTitle>AR Biblioteket - Login</PageTitle>

<div class="login-box">
	<p>Login</p>
	<form>
		<div class="user-box">
			<label>Email:</label>
			<input required="" name="" type="text"/>
		</div>
		<div class="user-box">
			<label>Password:</label>
			<input required="" name="" type="password"/>
		</div>
		<button>Submit</button>
	</form>
	<p>Har du ikke en konto? <a href="/sign_up">  Sign up!</a></p>
</div>

