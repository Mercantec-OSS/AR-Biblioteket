@page "/sign_up"

<PageTitle>AR Biblioteket - Sign up</PageTitle>

<div class="login-box">
	<p>Sign up</p>
	<form>
		<div class="user-box">
			<label>Email:</label>
			<input required="" name="" type="text" />
		</div>
		<div class="user-box">
			<label>Password:</label>
			<input required="" name="" type="password" />
		</div>
		<div class="user-box">
			<label>Access Key:</label>
			<input required="" name="" type="text" />
		</div>
		<button>Submit</button>
	</form>
	<p>Har du en konto? <a href="/login">Login her!</a></p>
</div>

